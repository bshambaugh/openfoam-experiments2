FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "100";
    object      k;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -2 0 0 0 0];

internalField   uniform 0.648;

boundaryField
{
    courseFace
    {
        type            calculated;
        value           uniform 0.648;
    }
    courseCyclicBoundary
    {
        type            cyclicAMI;
        value           uniform 0.648;
    }
    courseWalls
    {
        type            calculated;
        value           uniform 0.648;
    }
    freeBoundaryCourse
    {
        type            calculated;
        value           uniform 0.648;
    }
    courseSymmetryWalls
    {
        type            symmetryPlane;
    }
    fineSymmetryWall
    {
        type            symmetryPlane;
    }
    fineWalls
    {
        type            calculated;
        value           uniform 0.648;
    }
    fineCyclicBoundary
    {
        type            cyclicAMI;
        value           uniform 0.648;
    }
    fineplug
    {
        type            cyclicAMI;
        value           uniform 0.648;
    }
    faceFine
    {
        type            calculated;
        value           uniform 0.648;
    }
    inletFace
    {
        type            calculated;
        value           uniform 0.648;
    }
    inlet
    {
        type            calculated;
        value           uniform 0.648;
    }
    inletWalls
    {
        type            calculated;
        value           uniform 0.648;
    }
    outletInlet
    {
        type            cyclicAMI;
        value           uniform 0.648;
    }
}


